Cell[BoxData[{
 RowBox[{"Clear", "[", "FindAands", "]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"FindAands", "[", 
    RowBox[{"g_GradedBigraph", ",", "q_", ",", 
     RowBox[{"cylinder_:", "False"}]}], "]"}], " ", ":=", 
   RowBox[{
    RowBox[{"FindAands", "[", 
     RowBox[{"g", ",", "q", ",", "cylinder"}], "]"}], " ", "=", 
    "\[IndentingNewLine]", 
    RowBox[{"Module", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{
        RowBox[{"rank", "=", 
         RowBox[{"GraphRank", "[", "g", "]"}]}], ",", "n", ",", 
        RowBox[{"oldcandidate", "=", "0"}], ",", "candidate", ",", 
        RowBox[{"maxsteps", "=", "\[Infinity]"}], ",", "spokes"}], "}"}], ",",
       "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"If", "[", 
        RowBox[{"cylinder", ",", 
         RowBox[{"spokes", "=", 
          RowBox[{"1", "+", 
           RowBox[{"RankAtDepth", "[", 
            RowBox[{"g", ",", 
             RowBox[{"GraphDepth", "[", "g", "]"}]}], "]"}]}]}], ",", 
         RowBox[{"spokes", "=", "1"}]}], "]"}], ";", "\[IndentingNewLine]", 
       RowBox[{"(*", 
        RowBox[{
         RowBox[{"Print", "[", "spokes", "]"}], ";"}], "*)"}], 
       "\[IndentingNewLine]", 
       RowBox[{"For", "[", 
        RowBox[{
         RowBox[{"n", "=", "rank"}], ",", 
         RowBox[{"n", "\[LessEqual]", 
          RowBox[{"rank", "+", "maxsteps"}]}], ",", 
         RowBox[{"n", "++"}], ",", "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{"If", "[", 
           RowBox[{
            RowBox[{
             RowBox[{"Mod", "[", 
              RowBox[{"n", ",", "100"}], "]"}], "\[Equal]", "0"}], ",", 
            RowBox[{"DebugPrint", "[", 
             RowBox[{
             "\"\<Looking at rank \>\"", ",", " ", "n", ",", " ", 
              "\"\< while finding A and s.\>\""}], "]"}]}], "]"}], ";", 
          "\[IndentingNewLine]", 
          RowBox[{"candidate", " ", "=", " ", 
           RowBox[{"Expand", "[", 
            RowBox[{
             RowBox[{"q", "^", 
              RowBox[{"(", 
               RowBox[{
                RowBox[{"-", "rank"}], "-", "n"}], ")"}]}], 
             RowBox[{"(", " ", 
              RowBox[{
               RowBox[{"FindModifiedFj", "[", 
                RowBox[{"g", ",", "q", ",", "n", ",", "spokes"}], "]"}], "/.", 
               RowBox[{
                RowBox[{"q", "^", 
                 RowBox[{"(", 
                  RowBox[{"k_", "/;", 
                   RowBox[{"k", "<", "0"}]}], ")"}]}], "\[Rule]", "0"}]}], 
              ")"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
          RowBox[{"(*", 
           RowBox[{
            RowBox[{"Print", "[", "candidate", "]"}], ";"}], "*)"}], 
          "\[IndentingNewLine]", 
          RowBox[{"If", "[", 
           RowBox[{
            RowBox[{"And", "[", 
             RowBox[{
              RowBox[{"candidate", "\[Equal]", "oldcandidate"}], ",", 
              RowBox[{"CheckACandidate", "[", 
               RowBox[{"g", ",", "candidate", ",", "q", ",", "spokes"}], 
               "]"}]}], "]"}], ",", 
            RowBox[{
             RowBox[{"Return", "[", 
              RowBox[{"{", 
               RowBox[{"candidate", ",", 
                RowBox[{
                 RowBox[{"Max", "[", 
                  RowBox[{
                   RowBox[{"-", 
                    RowBox[{"LowestPower", "[", 
                    RowBox[{"candidate", ",", "q"}], "]"}]}], ",", "0"}], 
                  "]"}], "+", "1"}]}], "}"}], "]"}], ";"}]}], "]"}], ";", 
          "\[IndentingNewLine]", 
          RowBox[{"oldcandidate", "=", "candidate"}], ";"}]}], 
        "\[IndentingNewLine]", "]"}], ";", "\[IndentingNewLine]", 
       RowBox[{"(*", " ", 
        RowBox[{
         RowBox[{"Find", " ", "A"}], ",", " ", 
         RowBox[{
          RowBox[{"then", " ", "s", " ", "is", " ", "max", 
           RowBox[{"{", 
            RowBox[{
             RowBox[{"-", 
              RowBox[{"(", 
               RowBox[{"smallest", " ", "exponent", " ", "of", " ", "A"}], 
               ")"}]}], ",", "0"}], "}"}]}], " ", "+", "1"}]}], " ", "*)"}], 
       "\[IndentingNewLine]", 
       RowBox[{"Message", "[", "\"\<Not found.  Try more steps?\>\"", "]"}], 
       ";", "\[IndentingNewLine]", 
       RowBox[{"Return", "[", "$Failed", "]"}], ";"}]}], 
     "\[IndentingNewLine]", "]"}]}]}], ";"}], "\[IndentingNewLine]", 
